{
    "manifest_version": 2,
    "name": "One Main Thing",
    "version": "0.2.1",

    "author": "Matan Kotler-Berkowitz and Noam Raz",
    "description": "A simpler to-do list",

    "chrome_url_overrides" : {
        "newtab": "pages/newtab/newtab.html"
    },

    "permissions": [
        "storage",
         "tabs"
    ],

    "icons": {
        "128": "assets/icon/icon128.png"
    },
    "background": {
      "scripts": [
        "background/oninstall.js"
      ]
    }
}
